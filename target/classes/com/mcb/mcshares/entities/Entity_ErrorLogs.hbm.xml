<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mcb.mcshares.entities.Entity_ErrorLogs" table="error_logs"
           catalog="mcsharesbd">
            <id column="error_id" name="errorId">
            <generator class="identity"></generator>
        </id>
        <property name="errorCode">
            <column name="error_code" sql-type="varchar" />
        </property>
        <property name="errorMessage">
            <column name="error_message" sql-type="varchar"/>
        </property>
        
    </class>
</hibernate-mapping>